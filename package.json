{
    "name": "backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:run": "npm run build && npm run typeorm migration:run -- -d ./ormconfig.ts",
        "migration:generate": "npm run build && npm run typeorm migration:generate -- -d ./ormconfig.ts -p",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:migration": "npm run migration:run && npm run start:dev",
        "start:debug": "nest start --debug --watch",
        "start:prod": "npm run migration:run && node dist/src/main.js",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage --coverageReporters=\"json-summary\"",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest.config.json"
    },
    "dependencies": {
        "@nestjs/axios": "^4.0.0",
        "@nestjs/common": "^11.0.0",
        "@nestjs/core": "^11.0.0",
        "@nestjs/cqrs": "^11.0.0",
        "@nestjs/event-emitter": "^3.0.1",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/mapped-types": "^2.1.0",
        "@nestjs/microservices": "^11.0.12",
        "@nestjs/platform-express": "^11.0.0",
        "@nestjs/schedule": "^6.0.0",
        "@nestjs/throttler": "^6.4.0",
        "@nestjs/typeorm": "^11.0.0",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.7",
        "axios": "^1.8.4",
        "bcryptjs": "^3.0.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.7",
        "csrf-csrf": "^3.1.0",
        "dotenv": "^16.4.5",
        "helmet": "^8.1.0",
        "multer": "^2.0.2",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "uuid": "^11.0.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
